@prefix : <http://aida.kmi.open.ac.uk/ontology#> .
@prefix csos: <http://cso.kmi.open.ac.uk/schema/cso#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix orgz: <http://www.w3.org/ns/org#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

#################################################################
####    Properties #################################################
#################################################################
### paper Property
:hasCitationDistribution rdf:type owl:ObjectProperty; 
rdfs:domain  :paper; 
rdfs:range   :GRIDTypeDistribution.

:hasGridTypeDistribution rdf:type owl:ObjectProperty; 
rdfs:domain  :paper; 
rdfs:range   :CitationsDistribution.

:hasReference rdf:type owl:ObjectProperty;
rdfs:domain  :paper;
rdfs:range   :paper.
 
### properties with Literal Range
:hasJourName rdf:type owl:ObjectProperty; 
rdfs:domain  :paper; 
rdfs:range   rdfs:Literal.

:hasConfSeries rdf:type owl:ObjectProperty; 
rdfs:domain  :paper; 
rdfs:range    rdfs:Literal.

:hasConfName rdf:type owl:ObjectProperty; 
rdfs:domain  :paper; 
rdfs:range    rdfs:Literal.

:hasYear rdf:type owl:ObjectProperty; 
rdfs:domain  :paper; 
rdfs:range    rdfs:Literal.

:hasEntityType rdf:type owl:ObjectProperty; 
rdfs:domain  :paper; 
rdfs:range    rdfs:Literal.

:hasType rdf:type owl:ObjectProperty; 
rdfs:domain  :paper; 
rdfs:range    rdfs:Literal.

:hasIndustrialSector rdf:type owl:ObjectProperty; 
rdfs:domain  :paper; 
rdfs:range    rdfs:Literal.




### CitationsDistribution Property
:hasCitationByYear rdf:type owl:ObjectProperty; 
rdfs:domain  :CitationsDistribution; 
rdfs:range   rdfs:Literal.


:hasCitationByYear-weight rdf:type owl:DataTypeProperty; 
rdfs:domain  :CitationsDistribution; 
rdfs:range   xsd:float.
#### GRIDTypeDistribution  Property
:hasGridType rdf:type owl:ObjectProperty; 
rdfs:domain  :GRIDTypeDistribution; 
rdfs:range   rdfs:Literal.


:hasGridType-weight rdf:type owl:DataTypeProperty; 
rdfs:domain  :GRIDTypeDistribution; 
rdfs:range   xsd:float.



#################################################################
####    Classes #################################################
#################################################################
:paper rdf:type owl:Class.
:paper  owl:equivalentClass :aida:paper. 
:paper  owl:equivalentClass :mag:paper.
## Distributions ################
:CitationsDistribution rdf:type owl:Class; 
                        owl:unionOf(
  [rdf:type owl:Restriction;
  owl:onProperty :hasCitationByYear; 
  owl:allValuesFrom  rdfs:Literal] ## restrictions on literals
    [rdf:type owl:Restriction;  ## restrictions on float 
    owl:onProperty :hasCitationByYear-weight;
    owl:allValuesFrom    [ rdf:type   rdfs:DataType ; 
                           owl:onDataType xsd:float
    ]
    ]                          
).

### GRIDTypeDistribution :hasGridType  :hasGridType-weight

:GRIDTypeDistribution rdf:type owl:Class; 
                        owl:unionOf(
  [rdf:type owl:Restriction;
  owl:onProperty :hasGridType; 
  owl:allValuesFrom  rdfs:Literal] ## restrictions on literals
    [rdf:type owl:Restriction;  ## restrictions on float 
    owl:onProperty :hasGridType-weight;
    owl:allValuesFrom    [ rdf:type   rdfs:DataType ; 
                           owl:onDataType xsd:float
    ]
    ]                          
).
